.img-add {
	height: 196px;
	position: fixed;
	bottom: 0px;
	width: 100%;
}
.img-add .img {
	height: 156px;
	text-align: center;
	padding-top: 36px;
}
.img-add .img p {
	color: #666666;
}
.img-add .img img {
	width: 65px;
	height: 65px;
}
.img-add input {
	width: 100%;
	height: 40px;
	border-radius: 0px;
	border: 0px;
	border-top: 1px solid #66CC99;
	color: #66CC99;
}