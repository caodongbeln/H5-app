.mui-bar {
	background-color: #4a5c74;
}
.mui-content,.mui-table-view{
	background-color: #F6F6F6;
}
/*后退按钮*/

.back_x {
	float: right;
	width: 15px;
	height: 15px;
	display: block;
	background-image: url(../images/back_x.png);
	background-position: center;
	background-size: 100% 100%;
	background-repeat: no-repeat;
	margin-top: 14px;
}
.mui-content>.mui-table-view:first-child{
	margin-top: 0;
}
.activity_type li:first-child{
	height: 44px;
	line-height: 44px;
	color: #666;
	text-align: center;
}
.activity_type span{
	width: 33%;
	display: inline-block;
	height: 51px;
	background-color: #fff;
	border: 1px solid #f0f0f0;
	text-align: center;
	line-height: 51px;
	color: #999;
	font-size: 14px;
}
.activity_type li{
	border: none;
}
.activity_type_active{
	border-bottom:1px solid #66cc99 !important;
	color: #66CC99 !important;
}
.mui-table-view-cell{
	height: 44px;
}
/*发布活动类型结束*/

.mui-table-view-cell>a:not(.mui-btn) {
	margin: 0;
}
/*第二个ul的每个li右面的字体*/

.detail {
	float: right;
	margin-right: 34px;
}

.mui-input-numbox {
	vertical-align: top;
}
.mui-table-view-cell {
	line-height: 51px;
	border: 1px solid #F0F0F0;
	color: rgb(153, 153, 153);
	height: 51px;
	
}
.mui-content ul:nth-child(2) li,.mui-content ul:nth-child(3) li{
	background-color: #fff;
	padding-left: 10px;
}
input[type=text]{
	width: 60%;
	border: none;
	padding: 0;
	margin-bottom: 0;
	font-size: 14px;
}
.mui-table-view-cell>a:not(.mui-btn){
	padding:0;
}
textarea {
	/*border: 1px solid rgb(153, 153, 153);*/
	border: none;
	border-radius: 0;
	padding: 10px;
	/*min-height: 113px;*/
	font-size: 12px;
	color: #666;
}

/*上传图片*/
.add_imgBox{
	padding: 0 10px;
	margin-bottom: 35px;
}
.add_imgBox>img{
	width: 22%;
	margin-right: 10px;
}
.add_img{
	width: 18.9%;
	margin: 0 auto;
	margin-bottom: 20px;
	display: inline-block;
}
.add_img img{
	width: 100%;
}
.mui-btn{
	padding: 0;
}
.mui-btn-block{
	width: auto;
	height: 40px;
	margin: 0 10px 10px 10px;
	font-size: 15px;
	line-height: 38px;
	padding: 0;
}
.mui-btn-success{
	border-radius: 0px !important;
    border: 0px !important;
    border-top: 1px solid #66cc99 !important;
    height: 40px !important;
    color: #66cc99 !important;
    font-size: 15px !important;
    position: fixed !important;
    bottom: 0px !important;
    width: 100%;
}
.mui-btn-success{
	border: 1px solid rgb(102,204,153);
}

