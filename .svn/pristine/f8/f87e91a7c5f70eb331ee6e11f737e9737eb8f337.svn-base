.dingwei_height .mui-table-view-cell{
	border-bottom: 1px solid #dbdbdb;
}
.dingwei_height .mui-table-view-cell:last-child .mui-navigate-right:after{
	content: '\e583';
	color: #999999;
	right: 0;
	font-size: 20px;
}
.dingwei_height .mui-table-view-cell:first-child{
	padding: 5px 8px;
}
.mui-table-view-cell p:nth-child(1){
	margin-left: -2px;
}
::-webkit-input-placeholder {
	/* WebKit browsers */
	font-size: 15px;
}

:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	font-size: 15px;
}

::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	font-size: 15px;
}

:-ms-input-placeholder {
	/* Internet Explorer 10+ */
	font-size: 15px;
}