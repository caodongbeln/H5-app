body {
    	background: #484848;
    }
.mui-bar-nav {
     -webkit-box-shadow: 0 0px #999999; 
	 box-shadow: 0 0px #999999; 
}
.mui-bar {
	background: #484848;
	height: 52px;
	line-height: 52px;
}
.mui-bar .mui-title {
	color: #FFFFFF;
	height: 52px;
	line-height: 52px;
}
.message_list {
	background: #484848;
	color: #FFFFFF;
	margin-top: 8px !important;
	margin-bottom: 55px;
}
.message_list:before {
	background-color: transparent !important;
}
.message_list:after {
	background-color: transparent !important;
}
.message_list li {
	padding-top: 14px;
	border-bottom: 1px solid #999999;
    margin: 0px 10px;
}
.message_list li:first-child {
	border-top: 1px solid #999999 !important;
}
.message_list li:after {
	background-color: transparent !important;
}
.message_list .mui-media-object{
	border-radius: 30px;
	border: 1px solid #FFFFFF;
}
.message_list a {
	margin:-11px -20px !important;
}
.message_list .mui-media-body {
	font-size: 14px;
}
.message_list .mui-media-body p {
	font-size: 12px;
	/*color: #999999;*/
	color: #FFFFFF !important;
	margin-top: 3px;
}
.message_list .mui-media-body span {
	font-size: 12px;
	color: #999999;
}
.mui-active {
	background-color: transparent !important;
}
.message_list .content {
	font-size: 12px;
	color: #FFFFFF;
	/*padding: 0px 10px;*/
	word-wrap: break-word;
	margin-top: 3px;
}
.evaluate-close {
	height: 49px;
	text-align: center;
	line-height: 60px;
	background: #484848;
	width: 100%;
	position: fixed;
	bottom: 0px;
	margin-top:50px
}
.evaluate-close img{
	height: 22px;
	width: 22px;
}
