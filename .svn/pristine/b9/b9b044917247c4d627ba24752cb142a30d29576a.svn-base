		<script src="../js/functions.js" type="text/javascript" charset="utf-8"></script>
		<script type="text/javascript" src="../template/activity_detail.min.js"></script>
		<script src="../js/delArray.js" type="text/javascript" charset="utf-8"></script>
		<script src="../js/activity_detail.js" type="text/javascript" charset="utf-8"></script>
		<script type="text/javascript" charset="utf-8">
			new zs.Init('ActivityDetail');
		</script>
		<script type="text/javascript" charset="utf-8">
			mui.init({
				swipeBack: true //启用右滑关闭功能
