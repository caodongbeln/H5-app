.mui-content{
	width: 100%;
	height: 100%;
}
.activity-alert {
	background: #FFFFFF;
	position: absolute;
	top: 30%;
	z-index: 200;
	width: 70%;
	left: 15%;
	border-radius: 8px;
}
.activity-alert ul{
	margin-bottom: 5px !important;
}
