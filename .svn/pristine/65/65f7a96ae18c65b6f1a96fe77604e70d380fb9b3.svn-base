<div class="simplify-dynamic-box" data-id='{{id}}'>
	<div class="Head-portrait mui-pull-left" data-href='../tpl/personal.html' data-view='personal'>
		<img src="../images/cover.png" data-src="{{userInfo.cover}}" data-css='a50' />
	</div>
	<div class="simplify-dynamic">
		<p style="display: table;width: 100%;">
			<span id="" class="username color_333">{{userInfo.username}}</span>
			<span id="" class="release-time mui-pull-right">{{deal_time}}</span>
		</p>
		<p style="display: table;width: 100%;">
			<span id="" class="distance-release font_12">{{distance}}</span>
			<span id="" class="positioning-release mui-ellipsis font_12">{{addr}}</span>
			<i id="" class="positioning-icon background mui-pull-right" data-href='../tpl/dynamic_position_map.html' data-view='dynamic_position_map' data-extras='{"releasePosition":{{releasePosition}}}'></i>
		</p>
		<p class="my-speak-content">{{msg}}</p>
		{{if isarray == undefined}} {{else if isarray == 1}}
		<div class="picture-box">
			<img src="../images/default_back_cover.jpg" data-preview-src="" data-preview-group="{{isarray}}" data-src="{{imgs[0]}}" />
		</div>
		{{else if isarray > 1}}
		<div class="picture-box">
			<ul class="mui-table-view mui-grid-view mui-grid-9 release-images-box myimg">
				{{each imgs as val ii}}
				<li class="mui-table-view-cell mui-media mui-col-xs-4">
					<div class="fabu-img-box">
						<img src="../images/default_back_cover.jpg" data-preview-src="" data-preview-group="{{isarray}}" data-src="{{val}}" />
					</div>
				</li>
				{{/each}}
			</ul>
		</div>
		{{/if}}
	</div>
	<p class="dianzan-talk-share">
		<i id="" class="fenxiang-dianji-icon background" data-share="" data-fenxiang='{{id}}'></i>
		<i id="dianzan-dianji" class="dianzan-dianji-icon{{if is_like==1}}-active{{/if}} background" data-dianzan='{{id}}' data-uid='{{uid}}'></i>
		<i id="" class="pinglun-dianji-icon background" data-pinlun='{{id}}'></i>
	</p>

	<div id="dianzanqu">
		{{include './headerPortrait' myDynamicList}}
	</div>

</div>
<p style="height: 10px;background-color: #F0EFF4;"></p>

<div id="pinlunqu">
	{{include './pinlunqu' myDynamicList}}
</div>