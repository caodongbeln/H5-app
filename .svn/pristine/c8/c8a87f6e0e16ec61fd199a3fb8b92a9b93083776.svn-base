.mui-title{
	color: #fff;
}
.sms-code-box{
	height: 44px;
	border: 1px solid #d7d7d7;
	padding-left: 7px;
	padding-top: 9px;
	margin-bottom: 10px;
	width: 69.5%;
	display: inline-block;
}
.sms-code-box input{
	outline: none;
	font-size: 12px;
	line-height: 12px;
	height: 12px;
	border: none;
	background-color: transparent;
	width: 80%;
	padding-left: 10px;
	margin-bottom: 0;	
}
.sms-code{
	width: 14.5px;
	height: 19px;
	display: inline-block;
	background-image: url(../images/sms-code.png);
	vertical-align: middle;
}
.send-code{
	height: 44px;
	line-height: 44px;
	width: 29%;
	display: inline-block;
	background-color: #028ce6;
	color:#fff;
	vertical-align: middle;
	font-size: 12px;
	text-align: center;
}
