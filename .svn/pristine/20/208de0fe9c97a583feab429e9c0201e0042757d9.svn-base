.mui-bar {
	background: #4a5c74;
}
.mui-bar a,.mui-title {
	color: #FFFFFF;
}
/*修改个人资料首页*/
.mui-content {
	overflow-x:hidden;
}
.perfect-data-header {
	height: 100px;
	text-align: center;
	background: #FFFFFF;
}
.perfect-data-header img {
	height: 50px;
	width: 50px;
	border-radius: 30px;
	margin-top: 25px;
}
.perfect-data-input {
	height: 52px;
	background: #FFFFFF;
	margin-bottom: 13px;
}
.perfect-data-input span {
	width: 74.6%;
	margin-left: 13%;
	display: inline-block;
	height: 25px;
	margin-top: 13px;
}
.selected-sex {
	text-align: center !important;
}

.selected input {
	color: #66CC99 !important;
	
}
.selected{
	border-bottom: 1px solid #66CC99 !important;
}
.perfect-data-input span input {
	width: 45.7%;
	border: 1px solid #b4b4b4;
	border-radius: 0px;
	color: #999999;
	height: 25px;
	line-height: 10px;
}
.perfect-data-input span input:first-child {
	margin-right: 18px;
}
.perfect-data-input .nickname{
	width: 50%;
	margin-left: 25%;
	text-align: center;
	border-radius: 0px;
	border: 0px;
	height: 100%;
	font-size: 14px;
}
.perfect-data-choose li{
	height: 52px;
	background: #FFFFFF;
	font-size: 14px;
	color: #666666;
	line-height: 52px;
	margin-bottom: 1px;
	padding:0px 10px;
}
.perfect-data-choose li a{
	color: #999999;
	line-height: 52px;
} 
.perfect-data-choose li span {
	margin-top: 14px;
}
.perfect-data-choose li span .mui-switch{
	width: 45px;
	height: 23px;
}
.perfect-data-choose li span .mui-switch-handle{
	height: 20px;
}
.perfect-data-lang ul{
	/*height: 52px;*/
	background: #FFFFFF !important;
	border:0px !important;
	padding-left: 10px !important;
}
.perfect-data-lang ul li{
	padding: 0px !important;
    height: 52px;
    border: 0px !important;
    margin-top: 11px !important;
    margin-right: 10px !important;
}
.perfect-data-lang li:last-child {
	margin-right: -20px !important;
}
.perfect-data-lang ul li input{
	width: 96%;
    font-size: 12px;
    color: #999999;
    border-radius: 0px;
    height: 25px;
    line-height: 0px;
    margin-top: 3px;
}
.foreigner-cate {
	position: absolute;
    margin-top: 14px;
    margin-left: -20px;
    width: 9px;
    height: 5px;
}
.perfect-special-choose {
	color: #66CC99 !important;
	border: 0px !important;
}
.guide-service {
	margin-top: 13px;
	margin-bottom: 1px;
}
/*.guide-service li:last-child {
	line-height: 20px;
}*/
.perfect-switch-add li{
	height: 38px;
	background: #FFFFFF;
	line-height: 38px;
	color: #666666;
	font-size: 14px;
	padding: 0px 10px;
}
.perfect-switch-add li:last-child{
	line-height: 20px;
}
.perfect-switch-add li:last-child{
	color: #66CC99;
	font-size: 13px;
	padding-left: 20%;
}
.perfect-switch-add li span {
	margin-top: 8px;
}
.perfect-switch-add li span .mui-switch{
	width: 45px;
	height: 23px;
	border: 1px solid #DDDDDD !important;
	}
.perfect-switch-add li span .mui-switch-handle{
	height: 22px;
}


/*修改个人资料第二页*/
.perfect-data-select {
	background:transparent;
}
.perfect-data-select li {
	height: 52px;
	margin-bottom: 1px;
	background: #FFFFFF;
	font-size: 14px;
	color: #666666;
	padding: 0px 10px;
	line-height: 52px;
}
.perfect-data-select li span{
	color: #999999;
	position: ABSOLUTE;
    right: 0px;
}
.perfect-data-select a {
	line-height: 53.5px;
	color: #999999;
}

.perfect-data-title {
	height: 43px;
	text-align: center;
	font-size: 14px;
	line-height: 45px;
	color: #666666;
}
.perfect-hobby-cate ul {
	background-color: transparent !important;
	height: 43px;    	
}
.perfect-hobby-cate ul li{
	height: 43px;
	padding: 0px !important;
	/*border: 0px !important;*/
}
.perfect-hobby-cate ul li:first-child {
	margin-left: 0% !important;
}
.perfect-hobby-cate ul li input{
	height: 98%;
	width: 100%;
	border: 0px;
	border-radius: 0px;
	color: #999999;
}

