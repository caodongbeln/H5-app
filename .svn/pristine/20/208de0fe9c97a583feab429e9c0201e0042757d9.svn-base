.perfect-data-voice li:first-child img{
	margin-right: 30px;
}
.perfect-data-voice li:first-child p{
	margin-right: 20px;
}
.perfect-data-voice li:last-child {
	text-align: left !important;
}
.perfect-data-voice li:last-child img{
	width: 35px;
	height: 35px;
}
.perfect-data-voice img{
	width: 27px;
	height: 35px;
	margin-top: 15px;
}
.perfect-data-voice p{
	line-height: 25px;
	font-size: 14px;
	color: #666666 !important;
}

