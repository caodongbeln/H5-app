/*人物列表*/

.friend-list {
	position: relative;
	margin-bottom: 10px;
	width: 100%;
	display: table;
}

.friend-list p {
	position: absolute;
	bottom: 0;
	color: #fff;
	background-color: rgba(144, 142, 148, 0.5);
	width: 100%;
	text-align: center;
	font-size: 12px;
	margin-bottom: 0;
	line-height: 14px;
}

.friend-list li {
	width: 50px;
	height: 50px;
	/*float: left;*/
	display: inline-block;
	white-space: nowrap;
	margin-left: 10px;
	position: relative;
}

.friend-list li img {
	width: 100%;
	display: block;
}

.friend-list li:first-child {
	margin-left: 0px;
}

.color_999 {
	color: #999;
}

.send-dynamic {
	font-size: 14px;
	margin-right: 10px;
}


/*开关*/

.mui-switch.mui-active:before {
	content: '';
}

.mui-switch:before {
	content: '';
}

.mui-switch {
	width: 39px;
	height: 22px;
	border: 1px solid #ddd !important;
}

.mui-switch .mui-switch-handle {
	width: 19px;
	height: 20px;
	left: 0px !important;
}


