	width: 22px;
	height: 22px;
	display: block;
	margin: 0 auto;
	margin-top: 1px;
	margin-bottom: -2px;
}

.index {
	background-image: url(../images/index.png);
}

.msg {
	background-image: url(../images/msg.png);
}

