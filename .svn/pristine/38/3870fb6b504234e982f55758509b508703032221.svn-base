<!--生活照部分-->
<div class="General_lighting_box">
	<ul class="General_lighting">
		{{each uInfo.back_cover}}
		<li>
			<div class="">
				<img src="../images/default_header.png" data-src="{{$value}}" data-css='a50' data-preview-src="" data-preview-group="{{$index}}" />
			</div>
		</li>
		{{/each}}
	</ul>
</div>
<!--
            	作者：949961306@qq.com
            	时间：2016-06-02
            	描述：名称备注部分
            -->
<ul class="mui-table-view riqixingming personal-margin-top">
	<li class="mui-table-view-cell font_14 color_999 releative border-bottom">昵 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;称
		<span id="uName" class="color_333 riqixingming-span"></span>
		<span id="changeName" class="color_333 xiugaibeizhu absolute">修改备注</span>
	</li>
</ul>
<ul class="personal-margin-top background-color-white Personal-signature" style="margin-top: -10px !important;">
	<li class="font_14 color_999" style="margin-top: 10px;">个人签名</li>
	<li class="font_14 color_999" style="margin-top: 10px;"><textarea name="" rows="" cols="" placeholder="没有填写个性签名" disabled="disabled">{{uInfo.mood}}</textarea></li>
</ul>
<ul class="mui-table-view riqixingming" style="margin-top:1px ">
	<li class="mui-table-view-cell font_14 color_999 releative border-bottom">ID &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;号<span class="color_333 riqixingming-span">{{uInfo.id}}</span>
	</li>
</ul>
<ul class="mui-table-view riqixingming personal-margin-top">
	<li class="mui-table-view-cell font_14 color_999 border-bottom">性 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;别
		<span class="color_333 riqixingming-span">{{if uInfo.gender == 0}}女{{else}}男{{/if}}</span>
	</li>
	<li class="mui-table-view-cell font_14 color_999 border-bottom">出生日期<span class="color_333 riqixingming-span">{{if uInfo.birth == null || uInfo.birth == 0}}未设置{{else}}{{uInfo.birth}}{{/if}}</span></li>
	<li class="mui-table-view-cell font_14 color_999 border-bottom">手 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;机<span class="color_333 riqixingming-span">{{uInfo.mobile}}</span></li>
</ul>

