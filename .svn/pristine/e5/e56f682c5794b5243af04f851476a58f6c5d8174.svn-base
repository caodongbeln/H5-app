.mui-table-view-cell{
	padding: 10px;
}
.riqixingming-span{
	display: inline-block;
	margin-left: 10px;
}
.xiugaibeizhu{
	display: inline-block;
	padding: 5px;
	border: 1px solid #028ce6;
	right: 10px;
	top: 5px;
}

/*首页地图显示*/
.shouyedituxianshi {
	padding:6px 10px;
}
.shouyedituxianshi a{
	padding: 10px 15px !important;
}
.shouyedituxianshi .mui-ellipsis{
	/*margin-top: 18px;*/
}
.sangexuanxiang{
	height: 70px;
	/*padding-top: 10px;*/
}
.sangexuanxiang-div{
	margin-top: 10px !important;
}
.Interests_and_hobbies p{
	margin-bottom: 0;
}
/*生活地点右侧的图标*/
.personal-location-icon{
	width: 44px;
	height: 44px;
	display: inline-block;
	/*border: 1px solid red;*/
	top: 0;
	padding: 10px 13px;
	right:0;
	
}
.personal-location-icon img{
	width: 14px;
	height: 20px;
}
/*关联应用*/
.Associated-application,.Personal-signature{
	display: table;
	width: 100%;
}
.Associated-application li{
	float: left;
	/*padding-top: ;*/
	margin-right: 25px;
}
.Associated-application-name{
	/*display: inline-block;*/
	line-height: 12px;
	/*margin-top: -5px;*/
}
.Associated-application li:first-child{
	line-height: 70px;
	margin-right: 10px;
	margin-left: 10px;
}
.Associated-application-icon-renren,.Associated-application-icon-sina,.Associated-application-icon-ternea{
	margin-top: 4px;
	width: 50px;
	height: 50px;
	display: block;
	margin-bottom: -5px;
}
.Associated-application-icon-renren{
	background-image: url(../images/renren-icon.png);
}
.Associated-application-icon-sina{
	background-image: url(../images/sina-icon.png);
}
.Associated-application-icon-ternea{
	background-image: url(../images/tnrena-icon.png);
}
/*个人签名*/
.Personal-signature li{
	float: left;
}
.Personal-signature li:first-child{
	line-height: 70px;
	margin-left: 10px;
}
.Personal-signature li:last-child{
	min-height: 70px;
	width: 78%;
}
textarea{
	display: block;
	margin-bottom: 0;
	margin-top: 27px;
	padding-top: 0;
	padding-left: 10px;
	padding-right: 10px;
	padding-bottom: 0;
	font-size: 14px;
	line-height: 16px;
	border: none;
}
input[type="text"]{
	margin-bottom: 0;
	border: none;
	padding: 0 10px;
	height: auto;
}
textarea {
	color: #333333 !important;
}
.enjoy-input{
	width: 80% !important;
	/*padding: 10px;*/
	
} 
.mui-popup-inner{
	font-size: 14px;
	color: #333;
}
